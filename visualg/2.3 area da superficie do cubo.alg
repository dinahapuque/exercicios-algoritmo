algoritmo "semnome"
// Função :
// Autor :
// Data : 03/11/2015
// Seção de Declarações 
var
area, aresta: real
inicio
      escreval ("ÁREA DA SUPERFICIE DE UM CUBO")
      escreva ("Digite o tamanho da aresta do cubo: ")
      leia (aresta)
      
      area <- aresta*aresta*6
      
      escreva ("ÁREA DA SUPERFICIE: ",area)
fimalgoritmo
